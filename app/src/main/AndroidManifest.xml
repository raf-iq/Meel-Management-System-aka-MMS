<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="rafiq.com.example.app.mmsofflineversion">    <!-- To auto-complete the email text field in the login form with the user's emails -->
    <uses-permission android:name="android.permission.GET_ACCOUNTS" />
    <uses-permission android:name="android.permission.READ_PROFILE" />
    <uses-permission android:name="android.permission.READ_CONTACTS" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/cover_pic"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/cover_pic"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".QueryList"></activity>
        <activity android:name=".OptionsMenu" />
        <activity android:name=".MonthlyBazarDetail" />
        <activity android:name=".MakePayment" />
        <activity android:name=".ShowPayment" />
        <activity android:name=".Login" />
        <activity android:name=".BazarOfTheDay" />
        <activity android:name=".SetBazar" />
        <activity android:name=".MonthlyMealDetail" />
        <activity android:name=".PersonMealSet" />
        <activity android:name=".SetMeal" />
        <activity android:name=".Members" />
        <activity android:name=".Signin" />
        <activity android:name=".AddMember" />
        <activity android:name=".Profile" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>